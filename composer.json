{
  "name": "dave-liddament/php-language-extensions-phpstan-demo",
  "description": "Demo of PHP language extensions using PHPStan to validate",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "DaveLiddament\\PhpLanguageExtensionsPhpstanDemo\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Dave Liddament",
      "email": "dave@lampbristol.com"
    }
  ],
  "require-dev": {
    "phpstan/phpstan": "^1.10.15",
    "phpstan/extension-installer": "^1.3",
    "phpunit/phpunit": "^9.6.8",
    "phpstan/phpstan-phpunit": "^1.3.11",
    "dave-liddament/phpstan-php-language-extensions": "^0.3"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "require": {
    "dave-liddament/php-language-extensions": "^0.4"
  }
}
